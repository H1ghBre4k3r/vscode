{
  "name": "@kieler/keith-language",
  "version": "0.1.0",
  "description": "An extension for Theia connection language support for KEITH",
  "author": "Kiel University https://rtsys.informatik.uni-kiel.de/kieler",
  "license": "EPL-1.0",
  "keywords": [
    "keith-extension",
    "theia",
    "SCCharts",
    "language"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "build": "tsc && yarn lint",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@theia/application-package": "1.4.0",
    "@theia/getting-started": "1.4.0",
    "@theia/messages": "1.4.0",
    "@theia/monaco": "1.4.0",
    "@theia/navigator": "1.4.0"
  },
  "files": [
    "lib",
    "src",
    "build"
  ],
  "theiaExtensions": [
    {
      "frontend": "lib/browser/frontend-extension",
      "backend": "lib/node/backend-extension"
    }
  ]
}

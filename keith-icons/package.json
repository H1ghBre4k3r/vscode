{
  "name": "@kieler/keith-icons",
  "version": "0.1.0",
  "description": "An extension for Theia to provide icons for KEITH",
  "author": "Kiel University https://rtsys.informatik.uni-kiel.de/kieler",
  "license": "EPL-1.0",
  "keywords": [
    "keith-extension",
    "theia",
    "icons"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "build": "tsc && yarn lint",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@theia/core": "next"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "files": [
    "lib",
    "src"
  ],
  "theiaExtensions": [
    {
      "frontend": "lib/keith-icons-frontend-module"
    }
  ]
}

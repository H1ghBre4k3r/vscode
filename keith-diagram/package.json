{
  "name": "keith-diagram",
  "version": "0.1.0",
  "description": "An extension for Theia with diagram support for KEITH",
  "author": "Kiel University https://rtsys.informatik.uni-kiel.de/kieler",
  "license": "EPL-1.0",
  "keywords": [
    "keith-extension", 
    "theia",
    "diagram",
    "sprotty"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "build": "tsc && yarn lint",
    "watch": "tsc -w"
  },
  "dependencies": {
    "theia-sprotty": "0.1.20",
    "keith-language": "0.1.0",
    "keith-sprotty": "0.1.0",
    "sprotty": "0.4.2"
  },
  "files": [
    "lib",
    "src",
    "build"
  ],
  "theiaExtensions": [
    {
      "frontend": "lib/frontend-extension"
    }
  ]
}
